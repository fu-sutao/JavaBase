<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>


  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>servlet1</servlet-name>
    <servlet-class>com.main.Text</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>mysesson</servlet-name>
    <servlet-class>com.main.session.MySession</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
    <servlet-name>servlet1</servlet-name>
    <url-pattern>/text.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>mysesson</servlet-name>
    <url-pattern>/session.html</url-pattern>
  </servlet-mapping>
</web-app>
